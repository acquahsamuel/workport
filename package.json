{
  "name": "project-startup",
  "version": "1.0.0",
  "description": "workport job posting client",
  "main": "server.js",
  "scripts": {
    "start": "node backend/server",
    "server": "nodemon backend/server",
    "client": "npm start --prefix frontend",
    "dev": "concurrently \"npm run server\" \"npm run client\"",
    "data:import": "node backend/seeder",
    "data:destroy": "node backend/seeder -d",
    "heroku-postbuild": "NPM_CONFIG_PRODUCTION=false npm install --prefix frontend && npm run build --prefix frontend"
  },
  "keywords": [
    "startup",
    "for",
    "node",
    "codebase",
    "node",
    "nodeopen",
    "free-start"
  ],
  "author": "Acquah Samuel",
  "license": "MIT",
  "dependencies": {
    "bcrypt": "^5.0.1",
    "bcryptjs": "^2.4.3",
    "colors": "^1.4.0",
    "cookie-parser": "^1.4.4",
    "cors": "^2.8.5",
    "dotenv": "^8.1.0",
    "express": "^4.17.1",
    "express-fileupload": "^1.1.6-alpha.5",
    "express-mongo-sanitize": "^1.3.2",
    "express-rate-limit": "^5.0.0",
    "helmet": "^3.21.1",
    "hpp": "^0.2.2",
    "jsonwebtoken": "^8.5.1",
    "mongoose": "^5.7.1",
    "morgan": "^1.9.1",
    "node-sass": "^6.0.1",
    "nodemailer": "^6.3.0",
    "slugify": "^1.3.5",
    "xss-clean": "^0.1.1"
  },
  "engines": {
    "node": "10.0.0",
    "npm": "5.0.6"
  },
  "devDependencies": {
    "concurrently": "^5.3.0"
  }
}
